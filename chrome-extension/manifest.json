{
  "manifest_version": 3,
  "name": "Workspace Business Agent",
  "version": "0.1.0",
  "description": "Hello World agent for Google Workspace",

  "permissions": [
    "identity",
    "identity.email"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
